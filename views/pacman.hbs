            }
            else if (ghost_x[i] + 16 <= pac_x + 16 && ghost_x[i] + 16 >= pac_x) {
                if (ghost_y[i] <= pac_y + 16 && ghost_y[i] >= pac_y) {
                    console.log('death1')//death
                    pac_death()
                }
                else if (ghost_y[i] + 16 <= pac_y + 16 && ghost_y[i] + 16 >= pac_y) {
